package com.gmail.zak3;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n;
		System.out.print("Input n");
		n = sc.nextInt();
		for (int i = 0; i < n; i += 1) {
			for (int j = 0; j < n; j += 1) {
				if (j >= n - (i + 1)) {
					System.out.print("*");
				} else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
